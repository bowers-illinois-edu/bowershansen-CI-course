@article{follmann19941,
	Abstract = {Subjects in clinical trials are frequently randomized
		  within blocks to ensure balance between treatment groups
		  during the accrual period. Often, the block sizes
		  themselves are also randomly selected. If the trial is
		  unblinded and the block size known, the treatment
		  assignment of some subjects will be known before they are
		  randomized. This knowledge could lead to a bias where the
		  investigator selects healthier subjects for a particular
		  treatment. An investigator's potential to bias study
		  depends on two factors: knowledge of the block distribution
		  and the extent to which he uses this knowledge. Both
		  factors are investigated. Knowledge of the block
		  distribution is reflected by the variability of the
		  estimates of the distribution. Maximum likelihood estimates
		  (MLE) and ad hoc estimates are considered. An
		  investigator's willingness to bias is formalized by
		  defining a class of schemes where the decision to bias
		  depends on the probability of correctly guessing a
		  treatment assignment. The amount of bias these different
		  schemes can introduce is quantified. Fixed blocks are
		  easier to estimate and more susceptible to bias than random
		  blocks. In general, the block distribution with least bias
		  depends on the investigator's biasing strategy. However,
		  block distributions with larger block sizes and uniform or
		  increasing weights should work well in most situations. },
	Author = {Dean Follmann and Michael Proschan},
	Doi = {10.1016/0378-3758(94)90108-2},
	Issn = {0378-3758},
	Journal = {Journal of Statistical Planning and Inference},
	Keywords = {randomization},
	Number = {1},
	Pages = {1 - 17},
	Title = {The effect of estimation and biasing strategies on selection bias in clinical trials with permuted blocks},
	Url = {http://www.sciencedirect.com/science/article/pii/0378375894901082},
	Volume = {39},
	Year = {1994},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/0378375894901082},
