@Book{		  iver:05,
  address	= {New York},
  author	= {Iversen, Torben},
  optannote	= {"Iversen's analysis begins with the logic of worker
		  investment in specialized skills. He argues that those who
		  have acquired distinctive value-added skills will seek
		  policies to protect them; conversely, those protections
		  will lead individuals and firms to make the investments to
		  create those capacities. Workers with more general skills
		  demand and win fewer protections, which again creates a
		  self-perpetuating logic: The absence of the protection for
		  investment in skills results in lower levels of skill
		  investment. Using individual preferences as his primary
		  unit of analysis, Iversen creates exciting new ways to link
