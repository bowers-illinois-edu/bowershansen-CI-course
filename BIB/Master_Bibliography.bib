@Article{	  wood92,
  author	= {Wood, B. Dan},
  journal	= {American Journal of Political Science},
  number	= {1},
  optannote	= {Using the Clean Air Act as an empirical case, Wood
		  evaluates a "dynamic" federal policy system in which
		  national and subnational units hold joint regulatory
		  responsibility. Wood concludes in part that states appear
		  to adjust Clean Air Act policy and regulation in response
		  to local interest groups while the federal EPA is not
		  influenced substantially by interest groups. (Wood 92, at
		  60). However, Wood focuses on a dynamic federal scheme in
		  which simultaneous and overlapping authority is enjoyed by
		  both national and subnational units. Moreover, Wood's
		  empirical conclusions do not reflect on whether national
		  policymaking might be preferable even if interest group
		  influence were roughly equivalent at the state and national
		  levels. In practice, we might just want to cite Wood as
		  some empirical support for the claim that national is less
		  distorted than subnational.},
  pages		= {40--67},
  publisher	= {JSTOR},
  title		= {{Modeling Federal Implementation as a System: The Clean
		  Air Case}},
  volume	= {36},
  year		= {1992}
}

